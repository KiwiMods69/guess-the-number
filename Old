javascript:(function() {  let number = Math.floor(Math.random() * 100) + 1;  let guess;  let count = 0;    do {    guess = prompt("Guess a number between 1 and 100:");    count++;        if (guess > number) {      alert("Too high! Try again.");    } else if (guess < number) {      alert("Too low! Try again.");    } else {      alert("Congratulations! You guessed the number in " + count + " tries.");    }  } while (guess != number);})();
