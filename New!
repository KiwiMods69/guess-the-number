javascript:(function() {  let number = Math.floor(Math.random() * 100) + 1;  let guess;    do {    guess = prompt("Guess a number between 1 and 100:");        if (guess > number) {      document.body.style.backgroundColor = "rgb(255, " + (guess - number) * 3 + ", " + (guess - number) * 3 + ")";      alert("Too high! Try again.");    } else if (guess < number) {      document.body.style.backgroundColor = "rgb(" + (number - guess) * 3 + ", " + (number - guess) * 3 + ", 255)";      alert("Too low! Try again.");    } else {      document.body.style.backgroundColor = "rgb(255, 255, 0)";      alert("Congratulations! You guessed the number.");    }  } while (guess != number);})();
